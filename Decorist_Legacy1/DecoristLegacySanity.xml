<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="SanityTests" verbose="1" >
  <test name="LoginSanity" >
   <groups>
    <run>
       <include name="sanity"></include>
       <exclude name="regression"></exclude>
    </run>
   </groups>
   <classes>
       <class name="com.decoristLegacy.testcases.LoginTest" />
       <class name="com.decoristLegacy.testcases.LoginWithInvalidEmailTest" />
       <class name="com.decoristLegacy.testcases.LoginWithInvalidPasswordTest" />
       <class name="com.decoristLegacy.testcases.LoginWithoutCredentialsTest" />
   </classes>
  </test>
</suite>

